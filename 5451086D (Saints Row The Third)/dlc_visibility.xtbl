<TableDescription>
	<Name>VisibilityRules</Name>
	<Type>TableDescription</Type>
	<Element>
		<Name>Name</Name>
		<Type>String</Type>
		</Element>
	<Element>
		<Name>Offer_List</Name>
		<Type>Grid</Type>
		<Display_Name>Offer List</Display_Name>
		<Element>
			<Name>Offer_Visibility_Info</Name>
			<Type>Element</Type>
			<Display_Name>Offer Visibility Info</Display_Name>
			<Element>
				<Name>Name</Name>
				<Type>String</Type>
				</Element>
			<Element>
				<Name>OfferID</Name>
				<Type>String</Type>
				<Display_Name>Offer ID: 0x'0000000000000000'</Display_Name>
				<Description>The Offer ID</Description>
				<Default>5451086D</Default>
				<MaxLength>16</MaxLength>
				</Element>
			<Element>
				<Name>Bundle_ID</Name>
				<Type>Int</Type>
				<Display_Name>Bundle ID</Display_Name>
				<Description>If this bundle is licensed treat it as purchased by the user even if the fUserHasPurchased flag is not set.</Description>
				<Required>false</Required>
				<Default>0</Default>
				<MinValue>1</MinValue>
				<MaxValue>254</MaxValue>
				</Element>
			<Element>
				<Name>Indirect_License_Mask</Name>
				<Type>String</Type>
				<Display_Name>Indirect License Mask</Display_Name>
				<Description>Indirect License Mask in base 16.</Description>
				<Required>false</Required>
				<Default>00000000</Default>
				<MaxLength>8</MaxLength>
				</Element>
			<Element>
				<Name>Hidden_By_Default</Name>
				<Type>Selection</Type>
				<Display_Name>Hidden By Default</Display_Name>
				<Description>Is the offer hidden in the Xbox LIVE marketplace?</Description>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>Is_Season_Pass</Name>
				<Type>Selection</Type>
				<Display_Name>Is Season Pass</Display_Name>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>Requires_Season_Pass</Name>
				<Type>Selection</Type>
				<Display_Name>Requires Season Pass</Display_Name>
				<Description>Does the Season Pass need to be installed for this offer's content need to be consumed?</Description>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			<Element>
				<Name>Is_Online_Pass</Name>
				<Type>Selection</Type>
				<Display_Name>Is Online Pass</Display_Name>
				<Default>False</Default>
				<Choice>True</Choice>
				<Choice>False</Choice>
				</Element>
			</Element>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	<Element>
		<Name>Rule_List</Name>
		<Type>Grid</Type>
		<Display_Name>Rule List</Display_Name>
		<Description>Order is important! Rules are applied in order. A rule will not be applied if its key offer was hidden by a previous rule.</Description>
		<Element>
			<Name>Rule</Name>
			<Type>Element</Type>
			<Element>
				<Name>Key_Offer</Name>
				<Type>Reference</Type>
				<Reference>
					<File>..\..\tms\upload\dlc_visibility.xtbl</File>
					<Type>VisibilityRules.Offer_List.Offer_Visibility_Info.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Dependent_Offer</Name>
				<Type>Reference</Type>
				<Display_Name>Dependent Offer</Display_Name>
				<Reference>
					<File>..\..\tms\upload\dlc_visibility.xtbl</File>
					<Type>VisibilityRules.Offer_List.Offer_Visibility_Info.Name</Type>
					<OpenSeparate>False</OpenSeparate>
					</Reference>
				</Element>
			<Element>
				<Name>Visibility</Name>
				<Type>Selection</Type>
				<Choice>Show</Choice>
				<Choice>Hide</Choice>
				</Element>
			</Element>
		<Num_Display_Rows>8</Num_Display_Rows>
		</Element>
	<Single_Line_XML>False</Single_Line_XML>
	</TableDescription>

<EntryCategories>
	<Category>
		<Name>Entries</Name>
		</Category>
	</EntryCategories>
</root>